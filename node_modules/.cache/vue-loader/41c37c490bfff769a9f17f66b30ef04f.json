aElemento(){\r\n            this.amostraRascunho.elementos.push({\r\n                        \r\n                        componente:'',\r\n                        agente:''\r\n                    })\r\n        },\r\n        removeElemento(){\r\n            this.amostraRascunho.elementos.pop({\r\n                        componente:'',\r\n                        agente:''\r\n                    })\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nhr{\r\n    background-color: rgba(214, 214, 214, .5);\r\n}\r\n.imagemAmostra{\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\nimg{\r\n    padding: 5%;\r\n    height: 400px;\r\n    width: auto;\r\n}\r\n.file{\r\n    justify-content: center;\r\n}\r\n.salvar{\r\n    margin-top: 2%;\r\n}\r\n</style>"]}]}